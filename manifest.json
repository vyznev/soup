{
	"manifest_version": 2,
	"name": "SOUP",
	"version": "1.45.0",
	"permissions": ["tabs", "*://*.stackexchange.com/*", "*://*.stackoverflow.com/*", "*://*.superuser.com/*", "*://*.serverfault.com/*", "*://*.stackapps.com/*", "*://*.mathoverflow.net/*", "*://*.askubuntu.com/*"],
        "content_scripts": [{
            "matches" : ["*://*.stackexchange.com/*", "*://*.stackoverflow.com/*", "*://*.superuser.com/*", "*://*.serverfault.com/*", "*://*.stackapps.com/*", "*://*.mathoverflow.net/*", "*://*.askubuntu.com/*"],
            "js" : ["SOUP.user.js"]
        }],
        "browser_action": {
             "default_icon": "/icon/SOUP_icon_128.png"
        }
}
